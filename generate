    [38;5;214m<%=[m[38;5;231m[48;5;16m f.label [38;5;217m:name[m[38;5;231m[48;5;16m [38;5;214m%>[m[38;5;231m[48;5;16m[38;5;87m<[m[38;5;231m[48;5;16m[1m[38;5;227mbr[m[38;5;231m[48;5;16m[38;5;87m />[m[38;5;231m[48;5;16m
    [38;5;214m<%=[m[38;5;231m[48;5;16m f.text_field [38;5;217m:name[m[38;5;231m[48;5;16m [38;5;214m%>[m[38;5;231m[48;5;16m
